<div class="achievement-block">
	<div class="container">
		<div class="achievement-main-wrapper">
			<div class="heading">
				<h6>{{content.field_sub_heading.0['#context'].value}}</h6>
				<h2>{{content.field_heading.0['#context'].value}}</h2>
				<p>{{content.field_description.0['#context'].value}}</p>
			</div>
			<div class="row">
				{% for item in content.field_achievements['#items'] %}
					{% if item.entity.field_value.value %}
						<div class="col-12 col-md-6 col-lg-3">
							<div class="acvievement-card">
								<div class="acvievement-count" style="background-image: conic-gradient( #a12844 {{item.entity.field_value.value}}%, #ebebeb {{item.entity.field_value.value}}%, #ebebeb 100%);">
									<div class="acvievement-count-value">
										{{item.entity.field_value.value}}%
									</div>
								</div>

								<p>{{item.entity.field_stats_description.value}}</p>
							</div>
						</div>
					{% endif %}
				{% endfor %}
			</div>
		</div>
	</div>
</div>
