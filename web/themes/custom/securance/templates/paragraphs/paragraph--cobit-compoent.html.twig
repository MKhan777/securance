<div class="cobit-component-block">
	<div class="container">
		<div class="cobit-component-row">
			{% for item in content.field_cobit_section['#items'] %}
				<div class="cobit-component-col">
					<div class="cobit-component-content">
						<h2>{{ item.entity.field_image_caption.value }}</h2>
						<img src="{{ file_url(item.entity.field_image.entity.field_media_image.entity.uri.value) }}" alt="cobit-img">
					</div>
				</div>
			{% endfor %}
		</div>
	</div>
</div>
